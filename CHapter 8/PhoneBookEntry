public class PhoneBookEntryTest {

    public static String Name;
    private int Number;
    public PhoneBookEntryTest (String nameF, int pNumber)
    {
        Name = nameF;
        Number = pNumber;
    }
    public String toString ()
    {
        return  " The name is " + Name + " The number is " + Number + ".";
    }
    public boolean equals (Object other)
    {
        return (Name.equals(((PhoneBookEntryTest)other).getName()));
    }
    public int compareTo (Object other)
    {
        int result;
        String otherFirst = ((PhoneBookEntryTest)other).getName();
            result = Name.compareTo(otherFirst);
            return result;
    }
    public String getName ()
    {
        return Name;
    }
}
