import java.util.Scanner;
import java.util.ArrayList;
public class PhoneBookEntryTestDemo
{
    public static void main (String[] args)
    {
        Scanner scanner = new Scanner(System.in);
        ArrayList<PhoneBookEntryTest> transfer = new ArrayList<PhoneBookEntryTest>();
       //Declaring
        int i;
        //Declaring
        String Name;
        //Declaring
        int number;
            do
            {
                //Getting name
                System.out.print("Enter you last and first name. ");
                System.out.print("Seapreat them by a comma. ");
                Name = scanner.next();
               
                //Entering number
                System.out.print("");
                System.out.print("Enter the number");
                number = scanner.nextInt();
               
                //Adding stuff
                transfer.add (new PhoneBookEntryTest(Name, number));
               
                //Asking the user if he wants to make another list. 
                System.out.print("Enter 1 to make another list");
                 i = scanner.nextInt();
           
            }while (i == 1);
            for(PhoneBookEntryTest trans : transfer)
            System.out.println(trans);
    }
}
